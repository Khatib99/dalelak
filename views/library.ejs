    <link href="https://fonts.googleapis.com/icon?family=Material+Icons"
      rel="stylesheet">
</head>
<body>
	<div class="back">
		<a href="/student"><button class="back-button"><i id="icon" class="material-icons">arrow_back</i></button></a>

	</div>
	<div class="main-container">
		<div class="nav">
			<h3>My books</h3>
		</div>
		<div class="class-Section">

            <% for (var i =0; i < books.length;  i++ ) { %>
                <div class="section">
                    <div class="matiral">
                        <a target="_blank" href=<%= books[i].url %>><%= books[i].name %> - <%= books[i].material %></a>
                    </div>
                </div>
            <% } %>
		</div>
	</div>
</body>
</html>